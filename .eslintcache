 // Here we display the error and then remove the error query parameter from the location.\n        if(this.props.location.state && this.props.location.state.error) {\n            setTimeout(() => {\n                Alert.error(this.props.location.state.error, {\n                    timeout: 5000\n                });\n                this.props.history.replace({\n                    pathname: this.props.location.pathname,\n                    state: {}\n                });\n            }, 100);\n        }\n    }\n    \n    render() {\n        if(this.props.authenticated) {\n            return <Redirect\n                to={{\n                pathname: \"/\",\n                state: { from: this.props.location }\n            }}/>;            \n        }\n\n        return (\n            <div className=\"login-container\">\n                <div className=\"login-content\">\n                    <h1 className=\"login-title\">Login to your Recipe Stash</h1>\n                    <SocialLogin />\n                    <div className=\"or-separator\">\n                        <span className=\"or-text\">OR</span>\n                    </div>\n                    <LoginForm {...this.props} />\n                    <span className=\"signup-link\">New user? <Link to=\"/signup\">Sign up!</Link></span>\n                </div>\n            </div>\n        );\n    }\n}\n\nclass SocialLogin extends Component {\n    render() {\n        return (\n            <div className=\"social-login\">\n                <a className=\"btn btn-block social-btn google\" href={GOOGLE_AUTH_URL}>\n                    <img src={googleLogo} alt=\"Google\" /> Log in with Google</a>\n                {/* <a className=\"btn btn-block social-btn facebook\" href={FACEBOOK_AUTH_URL}>\n                    <img src={fbLogo} alt=\"Facebook\" /> Log in with Facebook</a>\n                <a className=\"btn btn-block social-btn github\" href={GITHUB_AUTH_URL}>\n                    <img src={githubLogo} alt=\"Github\" /> Log in with Github</a> */}\n            </div>\n        );\n    }\n}\n\n\nclass LoginForm extends Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            email: '',\n            password: ''\n        };\n        this.handleInputChange = this.handleInputChange.bind(this);\n        this.handleSubmit = this.handleSubmit.bind(this);\n    }\n\n    handleInputChange(event) {\n        const target = event.target;\n        const inputName = target.name;        \n        const inputValue = target.value;\n\n        this.setState({\n            [inputName] : inputValue\n        });        \n    }\n\n    handleSubmit(event) {\n        event.preventDefault();   \n\n        const loginRequest = Object.assign({}, this.state);\n\n        login(loginRequest)\n        .then(response => {\n            localStorage.setItem(ACCESS_TOKEN, response.accessToken);\n            Alert.success(\"You're successfully logged in!\");\n            this.props.history.push(\"/\");\n        }).catch(error => {\n            Alert.error((error && error.message) || 'Oops! Something went wrong. Please try again!');\n        });\n    }\n    \n    render() {\n        return (\n            <form onSubmit={this.handleSubmit}>\n                <div className=\"form-item\">\n                    <input type=\"email\" name=\"email\" \n                        className=\"form-control\" placeholder=\"Email\"\n                        value={this.state.email} onChange={this.handleInputChange} required/>\n                </div>\n                <div className=\"form-item\">\n                    <input type=\"password\" name=\"password\" \n                        className=\"form-control\" placeholder=\"Password\"\n                        value={this.state.password} onChange={this.handleInputChange} required/>\n                </div>\n                <div className=\"form-item\">\n                    <button type=\"submit\" className=\"btn btn-block btn-primary\">Login</button>\n                </div>\n            </form>                    \n        );\n    }\n}\n\nexport default Login\n","/Users/jasmine/Dropbox/Developer/ada/capstone/recipe-stash/src/user/signup/Signup.js",["78","79","80","81"],"/Users/jasmine/Dropbox/Developer/ada/capstone/recipe-stash/src/recipes/Recipes.js",["82","83","84","85"],"/Users/jasmine/Dropbox/Developer/ada/capstone/recipe-stash/src/recipes/AddRecipeForm.js",["86"],"/Users/jasmine/Dropbox/Developer/ada/capstone/recipe-stash/src/util/APIUtils.js",["87"],"/Users/jasmine/Dropbox/Developer/ada/capstone/recipe-stash/src/recipes/RecipeCard.js",["88"],"/Users/jasmine/Dropbox/Developer/ada/capstone/recipe-stash/src/common/LoadingIndicator.js",[],"/Users/jasmine/Dropbox/Developer/ada/capstone/recipe-stash/src/common/PrivateRoute.js",[],"/Users/jasmine/Dropbox/Developer/ada/capstone/recipe-stash/src/recipes/RecipeDetails.js",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"89","replacedBy":"93"},{"ruleId":"91","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":7,"column":31,"nodeType":"97","messageId":"98","endLine":7,"endColumn":32,"suggestions":"99"},{"ruleId":"100","severity":1,"message":"101","line":3,"column":27,"nodeType":"102","messageId":"103","endLine":3,"endColumn":44},{"ruleId":"100","severity":1,"message":"104","line":3,"column":46,"nodeType":"102","messageId":"103","endLine":3,"endColumn":61},{"ruleId":"100","severity":1,"message":"105","line":6,"column":8,"nodeType":"102","messageId":"103","endLine":6,"endColumn":14},{"ruleId":"100","severity":1,"message":"106","line":8,"column":8,"nodeType":"102","messageId":"103","endLine":8,"endColumn":18},{"ruleId":"100","severity":1,"message":"101","line":4,"column":27,"nodeType":"102","messageId":"103","endLine":4,"endColumn":44},{"ruleId":"100","severity":1,"message":"104","line":4,"column":46,"nodeType":"102","messageId":"103","endLine":4,"endColumn":61},{"ruleId":"100","severity":1,"message":"105","line":6,"colu